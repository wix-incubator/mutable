{
  "private": false,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "name": "typorama",
  "version": "0.0.330",
  "description": "data type system for javascript",
  "main": "dist/src",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wix-private/typorama"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build:clean": "npm run clean && npm run build",
    "build": "npm run build:src && npm run build:test && npm run build:testkit && npm run build:webpack",
    "build:src": "babel ./src/ -d ./dist/src/ --source-maps",
    "build:test": "babel ./test/ -d ./dist/test/ --source-maps",
    "build:testkit": "babel ./test-kit/ -d ./dist/test-kit/ --source-maps",
    "build:webpack": "webpack",
    "test:node": "mocha --reporter mocha-env-reporter ./dist/test.bundle.js",
    "test:browser": "karma start --single-run",
    "pretest": "npm run build:clean",
    "test": "npm run test:browser && npm run test:node",
    "start": "webpack-dev-server --progress --hot --inline --no-colors"
  },
  "author": "core3",
  "license": "c",
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.10.2",
    "babel-plugin-transform-es2015-modules-umd": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-preset-react": "6.11.1",
    "chai": "3.5.0",
    "karma": "1.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-env-reporter": "1.0.4",
    "karma-mocha": "1.1.1",
    "mocha": "2.5.3",
    "mocha-env-reporter": "1.0.2",
    "mocha-loader": "0.7.1",
    "rimraf": "2.5.2",
    "sinon": "git://github.com/cjohansen/Sinon.JS.git#b672042043517b9f84e14ed0fb8265126168778a",
    "sinon-chai": "2.8.0",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "lodash": "4.13.1",
    "escalate": "0.0.85"
  },
  "babel": {
    "presets": [
      "es2015-loose",
      "react"
    ],
    "plugins": [
      "transform-es2015-modules-umd",
      "transform-class-properties"
    ]
  }
}
