{
  "name": "mutable",
  "version": "1.0.17",
  "description": "Mutable state containers with dirty checking and more",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/src/index.js",
  "jsnext:main": "src/index.js",
  "homepage": "https://github.com/wix/mutable",
  "repository": {
    "type": "git",
    "url": "git@github.com:wix/mutable.git"
  },
  "bugs": {
    "url": "https://github.com/wix/mutable/issues"
  },
  "directories": {
    "test": "test"
  },
  "author": "Amir Arad <amira@wix.com> (http://github.com/amir-arad)",
  "contributors": [
    "Amir Arad <amira@wix.com> (http://github.com/amir-arad)"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build:clean": "npm run clean && npm run build:src",
    "build:src": "babel ./src/ -d ./dist/src/ --source-maps && babel ./test-kit/ -d ./dist/test-kit/ --source-maps",
    "bundle": "webpack",
    "pretest": "npm run bundle",
    "test:node": "mocha --reporter mocha-env-reporter ./dist/test.bundle.js",
    "test:browser": "karma start --single-run",
    "test": "npm run test:browser && npm run test:node",
    "start": "webpack-dev-server --progress --hot --inline --no-colors"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-preset-es2015": "6.13.2",
    "chai": "3.5.0",
    "karma": "1.1.2",
    "karma-chrome-launcher": "1.0.1",
    "karma-env-reporter": "1.0.4",
    "karma-mocha": "1.1.1",
    "mocha": "2.5.3",
    "mocha-env-reporter": "1.0.2",
    "mocha-loader": "0.7.1",
    "rimraf": "2.5.4",
    "sinon": "git://github.com/cjohansen/Sinon.JS.git#b672042043517b9f84e14ed0fb8265126168778a",
    "sinon-chai": "2.8.0",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "lodash": "4.14.1",
    "escalate": "0.0.92"
  },
  "babel": {
    "presets": [
      ["es2015", {"loose": true}]
    ],
    "plugins": [
      "transform-class-properties"
    ]
  }
}
