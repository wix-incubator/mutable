{
  "private": false,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "name": "typorama",
  "version": "0.0.65",
  "description": "data type system for javascript",
  "main": "dist/src",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wix/typorama"
  },
  "scripts": {
    "build:src": "./node_modules/.bin/babel ./src/ -d ./dist/src/ --modules umd --source-maps",
    "build:test": "./node_modules/.bin/babel ./test/ -d ./dist/test/ --modules umd --source-maps",
    "build:testkit": "babel ./test-kit/ -d ./dist/test-kit/ --modules umd --source-maps",
    "pretest": "npm run build:test && npm run build:testkit",
    "publish:tc": "./node_modules/.bin/ci",
    "test": "./node_modules/.bin/mocha --reporter $npm_config_reporter ./dist/test",
    "start": "./node_modules/.bin/webpack-dev-server --progress --hot --inline --no-colors"
  },
  "author": "core3",
  "license": "c",
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "bluebird": "^2.9.27",
    "chai": "^2.3.0",
    "core3-base": "1.5.19",
    "immutable": "^3.7.3",
    "mocha": "^2.2.5",
    "mocha-loader": "^0.7.1",
    "mocha-teamcity-reporter": "0.0.4",
    "node-libs-browser": "^0.5.2",
    "react": "^0.13.3",
    "sinon": "git://github.com/cjohansen/Sinon.JS.git#b672042043517b9f84e14ed0fb8265126168778a",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "lodash": "^3.9.3"
  }
}
